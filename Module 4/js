(function () {
    var names = ["Alex", "Andy", "Jennifer", "Michael", "Pauline", "Andrew", "Paula", "Paul", "Jenny", "Blake"];
    for (var name in names) {
       var firstLetter = names[name].charAt(0).toLowerCase();
        if (firstLetter === 'j') {
           byeSpeaker.speak(names[name]);
   } else {
     helloSpeaker.speak(names[name]);
   }
 }
 
 })();
